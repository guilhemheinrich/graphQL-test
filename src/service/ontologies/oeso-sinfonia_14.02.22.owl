        <rdfs:range rdf:resource="http://www.opensilex.org/vocabulary/oeso#Germplasm"/>
        <rdfs:comment xml:lang="en">germplasm of rootstock</rdfs:comment>
        <rdfs:comment xml:lang="fr">matériel génétique du porte-greffe</rdfs:comment>
        <rdfs:label xml:lang="fr">porte-greffe</rdfs:label>
        <rdfs:label xml:lang="en">rootstock</rdfs:label>
    </owl:ObjectProperty>
    


