            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://sinfonia.vignevin.com/oeso#hasRootstockGermplasm"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.opensilex.org/vocabulary/oeso#Germplasm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://sinfonia.vignevin.com/oeso#hasScionGermplasm"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.opensilex.org/vocabulary/oeso#Germplasm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://sinfonia.vignevin.com/oeso#Repetition"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://sinfonia.vignevin.com/oeso#PlantingDensity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A community of plants located on a defined area of a experimental plot, generally associated with only one experimental unit</rdfs:comment>
        <rdfs:comment xml:lang="fr">Un ensemble de plantes localisée sur une partie définie d&apos;une parcelle expérimentale, généralement associé à une et une seule unité expérimentale</rdfs:comment>
        <rdfs:label xml:lang="fr">placette</rdfs:label>
        <rdfs:label xml:lang="en">Subplot</rdfs:label>
    </owl:Class>
    


    <!-- http://sinfonia.vignevin.com/oeso#TastingRoom -->

